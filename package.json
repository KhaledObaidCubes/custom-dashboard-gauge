{
  "name": "@tsmesolutions/cubes-adafsa-custom",
  "version": "1.3.2",
  "description": "ADAFSA Custom Dashboard",
  "main": "dist/cubes-adafsa-custom.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "clean": "rimraf dist build",
    "npm:install": "npm i",
    "start": "npx webpack serve --open --hot --config webpack.dev.js --env vars=cubes.dev --mode development",
    "start:debug": "npx webpack serve --open --hot --config webpack.dev.js --env vars=cubes.dev --env devtool=source-map --mode development --stats-error-details",
    "build": "npx webpack --config webpack.dev.js  vars=cubes.dev --mode development --progress  --env devtool=source-map --profile --stats-error-details && tsc ",
    "build:prod": "npm run clean && tsc  && webpack --config webpack.prod.js --env vars=cubes.prod --env devtool=hidden-source-map  --mode production --progress --profile && tsc ",
    "update:deps": "npm update --save --save-dev",
    "release": "npm run build:prod && rimraf --glob dist/**/*LICENSE.txt dist/**/*js.map dist/dev dist/types "
  },
  "repository": "https://github.com/TSMESolutions/cubes-dashboard",
  "author": "TSME",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.23.3",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@types/node": "^20.11.20",
    "@types/ws": "^8.5.10",
    "autoprefixer": "^10.4.17",
    "babel-loader": "^9.1.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-preset-typescript-vue": "^1.1.1",
    "babel-preset-typescript-vue3": "^2.0.17",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "copyfiles": "^2.4.1",
    "core-js": "^3.36.0",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cubes": "npm:@tsmesolutions/cubes@^1.3.2",
    "cubes-app": "npm:@tsmesolutions/cubes-app@^1.15.7",
    "cubes-auth": "npm:@tsmesolutions/cubes-auth@^1.2.1",
    "cubes-dashboard": "npm:@tsmesolutions/cubes-dashboard@^1.11.1",
    "cubes-style": "npm:@tsmesolutions/cubes-style@^1.9.1",
    "cubes-ui": "npm:@tsmesolutions/cubes-ui@^2.8.13",
    "dotenv-webpack": "^8.0.1",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.6.0",
    "motion": "^10.17.0",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^5.0.5",
    "sass": "^1.71.1",
    "sass-loader": "^13.3.3",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "tslib": "^2.6.2",
    "typescript": "4.9.5",
    "url-loader": "^4.1.1",
    "vue": "^3.4.14",
    "vue-loader": "^17.4.2",
    "vue-router": "^4.3.0",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0",
    "whatwg-fetch": "^3.6.20",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "dependencies": {
    "core-js": "^3.35.0",
    "regenerator-runtime": "^0.14.1"
  }
}
